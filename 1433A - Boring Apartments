#include <bits/stdc++.h>
using namespace std;
 
int main(){
    int t=1; cin>>t;
    while(t--){
        string s;
        cin>>s;
        int len = s.size();
        int n = s[0]-'0';
        int ans = (n-1)*10;
        if(len == 1) ans += 1;
        if(len == 2) ans += 3;
        if(len == 3) ans += 6;
        if(len == 4) ans += 10;
        
        cout<<ans<<endl;
    }
    
    return 0;
}
