#include <iostream>
using namespace std;

int main()
{
    int x = 0;
    int n;
    int i;
    string s;

    cin >> n;
    for (i = 1; i <= n; i++)
    {
        cin >> s;

        if (s == "X++" || s == "++X")
        {
            x++;
        }
        else
        {
            x--;
        }
    }

    cout << x << endl;

    return 0;
}
