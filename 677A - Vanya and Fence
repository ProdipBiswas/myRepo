#include <iostream>
using namespace std;

int main()
{
    int n, h, i, count = 0, arr[1000];

    cin >> n >> h;
    for (i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    for (i = 0; i < n; i++)
    {
        if (arr[i] <= h)
        {
            count++;
        }
        else
        {
            count += 2;
        }
    }

    cout << count << endl;

    return 0;
}
