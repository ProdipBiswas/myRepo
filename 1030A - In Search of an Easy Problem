#include <iostream>
using namespace std;

int main()
{
    int n, i, count = 0;

    cin >> n;
    int arr[n];
    for (i = 0; i < n; i++)
    {
        cin >> arr[i];
        if (arr[i] == 1)
        {
            count++;
        }
    }
    if (count == 0)
    {
        cout << "easy" << endl;
    }
    else
    {
        cout << "hard" << endl;
    }

    return 0;
}
