#include <bits/stdc++.h>
using namespace std;
 
int main(){
    int t=1;
    cin>>t;
    while(t--){
        int a, b;
        cin>>a>>b;
        if(a>b){
            swap(a,b);
        }
        if(2*a>=b){
            cout<<4*a*a<<endl;
        }
        else{
            cout<<b*b<<endl;
        }
    }
    return 0;
}
