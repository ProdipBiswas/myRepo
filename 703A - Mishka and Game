#include <iostream>
using namespace std;

int main()
{
    int n, m, c;
    cin >> n;
    int i;
    int count = 0;
    for (i = 0; i < n; i++)
    {
        cin >> m >> c;
        if (m > c)
        {
            count++;
        }
        else if (m < c)
        {
            count--;
        }
    }
    if (count > 0)
    {
        cout << "Mishka";
    }
    else if (count < 0)
    {
        cout << "Chris";
    }
    else
    {
        cout << "Friendship is magic!^^";
    }

    return 0;
}
